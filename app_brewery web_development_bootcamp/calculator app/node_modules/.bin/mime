#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/calculator app/node_modules/mime/node_modules:/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/calculator app/node_modules:/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/node_modules:/home/zoran/coding_documents/Learn-code/node_modules:/home/zoran/coding_documents/node_modules:/home/zoran/node_modules:/home/node_modules:/node_modules:/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/calculator app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/calculator app/node_modules/mime/node_modules:/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/calculator app/node_modules:/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/node_modules:/home/zoran/coding_documents/Learn-code/node_modules:/home/zoran/coding_documents/node_modules:/home/zoran/node_modules:/home/node_modules:/node_modules:/home/zoran/coding_documents/Learn-code/app_brewery web_development_bootcamp/calculator app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
